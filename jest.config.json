{
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "esbuild-jest"
    },
    "silent": true,
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "dist/",
      "backup/",
      "backup_integration/",
      "coverage/lcov-report"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ]
}