{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../test/unit",
  "testRegex": ".*\\.test\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "esbuild-jest"
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}