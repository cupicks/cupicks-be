{
  "name": "22-04-express-ts",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "unchaptered",
  "license": "ISC",
  "description": "",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=prod node dist/server.js",
    "start:dev": "cross-env NODE_ENV=dev tsc-watch --onSuccess \"node dist/server.js\"",
    "test": "cross-env NODE_ENV=test jest --findRelatedTests test/ --silent",
    "test:watch": "cross-env NODE_ENV=test jest --findRelatedTests test/unit/ --runInBand --silent --watch",
    "test:unit": "cross-env NODE_ENV=test jest --findRelatedTests test/unit/ --silent",
    "test:unit:watch": "cross-env NODE_ENV=test jest --findRelatedTests test/unit/ --silent --watch",
    "test:int": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --findRelatedTests test/integration/ --silent",
    "test:int:watch": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --findRelatedTests test/integration/ --silent --watch",
    "test:e2e": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --findRelatedTests test/e2e/ --silent",
    "test:e2e:watch": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --findRelatedTests test/e2e/ --silent --watch",
    "test:cov": "cross-env NODE_ENV=test jest --findRelatedTests test/ --runInBand --coverage --silent",
    "test:clear": "jest --clearCache",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.161.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "express": "^4.17.3",
    "express-rate-limit": "^6.5.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "1.4",
    "multer-s3": "^3.0.1",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "@jest/types": "^28.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^15.14.9",
    "@types/prettier": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "eslint": "^8.22.0",
    "faker": "^5.5.3",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.3",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.8.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts}": "prettier --write"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "dist/",
      "backup/",
      "backup_integration/",
      "coverage/lcov-report"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ]
  }
}
