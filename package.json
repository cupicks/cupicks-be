  "license": "ISC",
  "description": "",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=prod node dist/server.js",
    "start:dev": "cross-env NODE_ENV=dev tsc-watch --onSuccess \"node dist/server.js\"",
    "test": "cross-env NODE_ENV=test jest --findRelatedTests test/unit/ --silent",
    "test:watch": "cross-env NODE_ENV=test jest --findRelatedTests test/unit/ --runInBand --watch --silent",
    "test:int": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --findRelatedTests test/integration/ --silent",
    "test:cov": "cross-env NODE_ENV=test jest --findRelatedTests test/ --runInBand --coverage --silent",
    "test:clear": "jest --clearCache"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-rate-limit": "^6.5.2",
    "joi": "^17.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^15.3.0",
    "@types/prettier": "^2.7.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "eslint": "^8.22.0",
    "faker": "^5.5.3",
    "morgan": "^1.10.0",
    "prettier": "^2.7.1",
    "tsc": "^2.0.3",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.3.4"
  }
}
